{
  "name": "mate-ai",
  "version": "1.0.0",
  "description": "Chat with OpenAI via messaging services",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "build": "webpack --mode production",
    "build:voice": "cd src/voice-call-server && npm run build",
    "build:all": "npm run build && npm run build:voice",
    "client": "webpack serve --mode development",
    "voice": "cd src/voice-call-server && npm run dev:watch",
    "ngrok": "node scripts/start-all-tunnels.js",
    "check": "node scripts/check-setup.js",
    "postsetup": "chmod +x scripts/start-all-tunnels.js && chmod +x scripts/check-setup.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\" \"npm run voice\"",
    "dev:full:ngrok": "concurrently \"npm run dev\" \"npm run client\" \"npm run voice\" \"npm run ngrok\"",
    "dev:full:auto": "node scripts/dev-with-dependencies.js",
    "setup": "npm install && cd src/voice-call-server && npm install && npm run check"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^7.6.3",
    "openai": "^4.20.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-phone-number-input": "^3.4.12",
    "styled-components": "^6.1.1",
    "twilio": "^4.23.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "nodemon": "^3.0.1",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
